inventory = ["Axe", "Great Sword", "Stick"]
rarity = {
  "Great Sword" : 98,
  "Golden Bow" : 97,
  "Iron Sword" : 80,
  "Axe" : 34,
  "Stick" : 1}
 
 sort_inv = sorted(inventory, key=rarity.__get.item__, reverse=True)
 print(sort_inv)
 # ["Great Sword", "Axe", "Stick"]
 
 print(rarity.__get.item__("Stick)
 # 1
